.Bookmarks
    padding: 30px 10px 0px 10px

    .singleBookmarks
        border-bottom: 1px solid #cecece87
        margin-bottom: 30px
        padding-bottom: 5px
        &:last-child
            border-bottom: 0px solid
            margin-bottom: 0px
    
    ul.Tags
        list-style-type: none
        padding-left: 0px
        li 
            display: inline-block
            padding: 5px 10px
            margin-right: 10px
            background: #1f68c1
            border-radius: 5px
            color: white
            font-size: 13px
            text-transform: uppercase
            font-weight: 600
            position:relative
            margin-bottom: 1px;
            .remove-icon
                width: 15px
                height: 15px
                float:right
                line-height: 1
                margin-left: 10px
                margin-right: 2px
                &:after
                    content: "\d7"
                    font-size: 20px
                    width: 100%
                    height: auto
.deleteColumn
    position: relative
    text-align: center

.deleteBookmarks
    position: absolute
    top: 50%
    transform: translateY(-50%)
    color: #bdc4c9
    font-weight: 600
    img
        margin-top: -5px
        padding-right: 10px

.notFound
    font-size: 20px
    color: #e6e6e6
    text-transform: uppercase
    text-align: center
    background-color: #1f68c1ad
    border-radius: 20px